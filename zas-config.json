{
    "directories": {
        "root": "zasenv",
        "supporters": "supporters",
        "actors": "actors"
    },

    "actors": {
        "foreground": "foreground",
        "background": "background"
    },

    "extensions":
    {
        "class": ".class.php",
        "interface": ".ifc.php",
        "trait": ".trait.php",
        "include": ".inc.php",
        "config": ".conf"
    },

    "path": {
        "class": "/supporters/classes/",
        "interface": "/supporters/interfaces/",
        "trait": "/supporter/traits/",
        "config": "/supporters/configs/"
    }
}