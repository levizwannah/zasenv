{
    "directories": {
        "root": "zasenv",
        "supporters": "supporters",
        "actors": "actors"
    },

    "actors": {
        "foreground": "foreground",
        "background": "background"
    },

    "extensions": {
        "class": "class.php",
        "abstractClass": "aclass.php",
        "interface": "ifc.php",
        "trait": "trait.php",
        "include": "inc.php",
        "config": "conf"
    },

    "path": {
        "class": "/supporters/classes/",
        "abstractClass": "/supporters/classes/abstract",
        "interface": "/supporters/interfaces/",
        "trait": "/supporter/traits/",
        "config": "/supporters/configs/"
    },

    "nameConventionsRegex": {
        "abstractClass": "^Abstract\\w+*$",
        "interface": "^\\w+Interface$",
        "trait": "\\w+Trait$",
        "class": ".*"
    }
    
}